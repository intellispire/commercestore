{
    "name": "intellispire/commercestore",
    "type": "wordpress-plugin",
    "description": "The easiest way to sell digital information with WordPress.",
    "keywords": ["cs", "cstore", "commercestore"],
    "homepage": "https://commercestore.com/",
    "license": "GPL-2.0+",
    "support": {
        "issues": "https://commercestore.com.com/support"
    },
    "config": {
        "disable-tls": true,
        "secure-http": false,
        "allow-plugins": {
            "composer/installers": true,
            "composer/package-versions-deprecated": true
        },
        "platform": {
          "php": "7.4.28"
        }
    },
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0",
        "timber/timber": "dev-master",
        "stripe/stripe-php": "^7.113"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "yoast/phpunit-polyfills": "1.x-dev",
        "humbug/php-scoper": "1.0.x-dev",
        "phpunit/php-invoker": "^3.1"
    },
    "minimum-stability": "dev",
    "autoload": {
      "psr-4": {
        "": ["csae/src", "csae-dev/src","src", "lib/" ]
      }
    }
}
